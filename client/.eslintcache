        \r\n    </div>\r\n    )\r\n}\r\n\r\nexport default Cell","C:\\Users\\Lucia\\Desktop\\minesweeper\\client\\src\\helpers\\showCell.js",["203"],"C:\\Users\\Lucia\\Desktop\\minesweeper\\client\\src\\helpers\\countCellsHidden.js",["204","205"],"export const countCellsHidden = (game) =>{\r\n    let table = game.table\r\n    let arr=[];\r\n\r\n    table.map(row=>{\r\n        return(\r\n            row.map(cell=>{\r\n                if(cell.show===false){\r\n                    arr.push({x:cell.x,y:cell.y})\r\n                }\r\n            })\r\n        )\r\n    })\r\n    return arr\r\n}\r\nexport const showAll = (game) =>{\r\n    let table = game.table\r\n    table.map(row=>{\r\n        return(\r\n            row.map(cell=>{\r\n                if(cell.value!=='x'){\r\n                    cell.show=true\r\n                }\r\n            })\r\n        )\r\n    })\r\n    return table\r\n\r\n}\r\n\r\n","C:\\Users\\Lucia\\Desktop\\minesweeper\\client\\src\\redux\\reducers\\gameReducer.js",["206"],"C:\\Users\\Lucia\\Desktop\\minesweeper\\client\\src\\components\\HeaderGame\\HeaderGame.jsx",[],"C:\\Users\\Lucia\\Desktop\\minesweeper\\client\\src\\redux\\actions\\gameActions.js",["207"],"C:\\Users\\Lucia\\Desktop\\minesweeper\\client\\src\\components\\HeaderGame\\Timer.jsx",["208","209"],"import React,{useState,useEffect} from 'react'\r\nimport './timer.css'\r\nimport { ReactComponent as Reloj } from '../../assets/reloj.svg';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport {saveTime,setStateGame} from '../../redux/actions/gameActions'\r\n\r\nconst Timer = ()=>{\r\n\r\nconst [seconds, setSeconds] = useState(0.5)\r\n    \r\nconst {state,time} = useSelector((store)=>store.gameReducer)\r\n\r\nconst dispatch = useDispatch()\r\nlet interval=null\r\n   \r\nfunction saveTimeGame(seconds){\r\n    dispatch(saveTime(seconds))\r\n}\r\n\r\nuseEffect(() => {\r\n    if(state===\"\") return setSeconds(0)\r\n    if(state===\"load\")  {\r\n        setSeconds(time)\r\n        saveTimeGame(0)\r\n        return\r\n    }\r\n    if(state===\"in progress\"){\r\n        interval = setInterval(() => {\r\n            setSeconds(seconds => seconds + 1)\r\n        }, 1000);\r\n    }\r\n    if(state===\"win\"||state===\"loss\"){\r\n        return saveTimeGame(seconds)\r\n    }\r\n    if(!state !==\"load\"){\r\n        saveTimeGame(seconds)   \r\n    }\r\n    return () => clearInterval(interval);\r\n    },[state]);\r\n    \r\nreturn(\r\n    <div className=\"time\">\r\n        <h2>{seconds===0?\"000\":((seconds/100).toFixed(2)).toString().replace(\".\",\"\")}</h2>\r\n        <Reloj className=\"time-reloj\"/>\r\n    </div>\r\n)\r\n\r\n}\r\n\r\nexport default Timer","C:\\Users\\Lucia\\Desktop\\minesweeper\\client\\src\\components\\Home\\Home.jsx",["210"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\index.js",[],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\App.js",[],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\redux\\store.js",[],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\components\\Home\\Home.jsx",["211","212"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\redux\\reducers\\gameReducer.js",["213","214","215","216","217","218"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\redux\\actions\\gameActions.js",["219"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\components\\Table\\Table.jsx",["220"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\components\\HeaderGame\\HeaderGame.jsx",["221","222","223"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\helpers\\createTable.js",["224","225"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\helpers\\showCell.js",["226"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\helpers\\countCellsHidden.js",["227","228"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\components\\HeaderGame\\Timer.jsx",["229","230","231"],"C:\\Users\\Lucia\\desktop\\minesweeper\\client\\src\\components\\Cell\\Cell.jsx",["232"],"C:\\User